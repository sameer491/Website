<div id="band" class="container text-center">
  <h3>Namaz Times</h3>
  <script type="text/javascript" src="../Website/PrayTimes.js"></script>
  
  
  
  <div align="center" id="table"></div>
  <script type="text/javascript">
	
	var date = new Date(); // today
	var set = prayTimes.setMethod('Tehran');
	var times = prayTimes.getTimes(date, [29,-95], -5, 0, '12h');
	var list = ['Imsak','Fajr', 'Sunrise', 'Dhuhr', 'Asr', 'Maghrib', 'Isha'];

	var html = '<table id="timetable">';
	html += '<tr><th colspan="2">'+ date.toLocaleDateString()+ '</th></tr>';
	for(var i in list)	{
		html += '<tr><td>'+ list[i]+ '</td>';
		html += '<td>'+ times[list[i].toLowerCase()]+ '</td></tr>';
	}
	html += '</table>';
	document.getElementById('table').innerHTML = html;

</script>

</div>

